	<div class="container">
	<h1>Sign In</h1>
	<%= form_with url: sessions_path do |form| %>
	  <div>
	    <%= form.label :email %>
	    <%= form.text_field :email %>
	  </div>
	  <div>
	    <%= form.label :password %>
	    <%= form.text_field :password, type: "password" %>
	  </div>
	  <%= form.submit "Sign In" %>
	<% end %>
</div>